#include<stdio.h>

int main()
{
	int i,j;
	int b[2][3];
	int a[3][2]={1,2,3,4,5,6};
	for(i=0;i<3;i++)
	{
		for(j=0;j<2;j++)
		printf("%d\t",a[i][j]);
		printf("\n");
	}
	for(i=0;i<3;i++)
		for(j=0;j<2;j++)
		b[j][i]=a[i][j];
	for(j=0;j<2;j++)
	{
		for(i=0;i<3;i++)
		printf("%d\t",b[j][i]); 
		printf("\n");
	}
	 return 0;
 } 
